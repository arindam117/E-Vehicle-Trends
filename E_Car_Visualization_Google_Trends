# AUTO COMPARISON PERSONAL PROJECT 
import pandas as pd
import numpy as np
import seaborn as sns
import matplotlib.pyplot as plt
%matplotlib inline

from pytrends.request import TrendReq  # THis will help fetch data from google trends 

# Creating a pytrend object to request data from google trends
pytrends = TrendReq(hl='en-us') # hl is host language 

# Extract data about keywords 

keywords = ['electric car','solar car','Hydrogen car','e-bike','petrol car']

pytrends.build_payload(keywords,timeframe = 'today 5-y') # The timeframe = 'today 5-y' gives data back to 5 years from today

data = pytrends.interest_over_time()

data.plot(kind='line')  

plt.show()
